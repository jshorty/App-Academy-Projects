<h1>Sign Up</h1>

<form class="" action=" <%= users_url %>" method="post">
  <%= auth_token %>

  <label>Username
    <input type="text" name="user[username]" value=" <%= @user.username %>">
  </label>

  <label>Password
    <input type="password" name="user[password]" value="">
  </label>

  <button>Submit</button>

</form>
