<!-- template renders in view with parent element of <form> -->
<h1><%= return (board.isNew() ? "New" : "Edit"); %> Board</h1>
<br>
<label for="board-title">Title</label>
<input type="text" name="board[title]"
    id="board-title" value="<%= board.get('title') %>">
<br>
<button id="board-form-submit">Submit</button>
